{
  "name": "Celestial Echoes Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#050615",
    "editor.foreground": "#eaecfb",
    "activityBar.background": "#0a0c29",
    "activityBar.foreground": "#2d40d2",
    "activityBarBadge.background": "#2d40d2",
    "activityBarBadge.foreground": "#eaecfb",
    "sideBar.background": "#050615",
    "sideBar.foreground": "#abb3ed",
    "sideBarTitle.foreground": "#2d40d2",
    "sideBarSectionHeader.background": "#0a0c29",
    "editorGroupHeader.tabsBackground": "#050615", // Example: Light Blue
    "tab.activeBackground": "#0a0c29",
    "tab.inactiveBackground": "#050615",
    "tab.activeForeground": "#2d40d2",
    "tab.inactiveForeground": "#5766db",
    "editorLineNumber.foreground": "#5766db",
    "editorLineNumber.activeForeground": "#2d40d2",
    "editorCursor.foreground": "#d12e80",
    "editor.selectionBackground": "#334499",
    "editor.inactiveSelectionBackground": "#0a0c2900",
    "editorIndentGuide.background1": "#2433a8",
    "editorIndentGuide.activeBackground1": "#2d40d2",
    "statusBar.background": "#2d40d2",
    "statusBar.foreground": "#eaecfb",
    "statusBar.debuggingBackground": "#d12e80",
    "statusBar.debuggingForeground": "#eaecfb",
    "titleBar.activeBackground": "#0a0c29",
    "titleBar.activeForeground": "#2d40d2",
    "titleBar.inactiveBackground": "#050615",
    "titleBar.inactiveForeground": "#5766db",
    "quickInput.background": "#050615", // Example: Dark gray background
    "quickInput.foreground": "#da5899", // Example: Light gray text in input box
    "quickInputList.focusBackground": "#0d1126", // Example: Dark blue background for selected item
    "quickInputList.focusForeground": "#bf9140",
    "menu.background": "#0a0c29", // Example: Dark gray background for the menu
    "menu.foreground": "#b3bbe6", // Example: Light gray text for menu items
    "menu.selectionBackground": "#1d237c", // Example: VS Code blue for selected item background
    "menu.selectionForeground": "#a72566", // Example: White text for selected item
    "menu.separatorBackground": "#bf9140", // Example: Medium gray for separators
    "list.hoverBackground": "#272fa5", // Example: Slightly lighter gray on hover
    "list.hoverForeground": "#edabcc", // Example: White text on hover

    "notifications.foreground": "#dde4fa",     // Your text color
    "notifications.background": "#030714",
    "notifications.border": "#8ca6ed",        // Your primary as a border
    "notificationsErrorIcon.foreground": "#e03d5c", // Your accent for errors
    "notificationsWarningIcon.foreground": "#bf9140", // Your secondary for warnings
    "notificationsInfoIcon.foreground": "#8ca6ed", // Your primary for info
    "notificationLink.foreground": "#8ca6ed"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5766db"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#bf9140"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#d12e80"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#4055bf"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#d12e80"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#2d40d2"
      }
    },
    {
      "name": "Class & Type",
      "scope": ["entity.name.type", "entity.name.class", "support.type"],
      "settings": {
        "foreground": "#4055bf"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#bf9140"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d12e80",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#5766db"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#da5899"
      }
    },
    {
      "name": "HTML/JSX Attribute Names",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#334499" // Blue color for attributes
      }
    },
    {
      "name": "HTML/JSX Div Names",
      "scope": "entity.name.tag.structure.div.html",
      "settings": {
        "foreground": "#6677cc" // Blue color for attributes
      }
    },

    {
      "scope": "markup.heading.markdown",
      "settings": {
        "foreground": "#d12e80", // Example: Gold color
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold.markdown",
      "settings": {
        "foreground": "#4055bf" // Example: Light Salmon color
      }
    },
    {
      "scope": "markup.italic.markdown",
      "settings": {
        "foreground": "#e382b3", // Example: Pale Green color
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#bf9140" // Example: Light Blue color
      }
    },
    // Markdown Links (text part)
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#edabcc", // Example: Light Sky Blue color
        "fontStyle": "underline"
      }
    },
    {
      "scope": "text.html.markdown", // Use the scope you found
      "settings": {
        "foreground": "#abb3ed" // Example: A light gray color
      }
    }
    // Add more rules for
  ]
}
